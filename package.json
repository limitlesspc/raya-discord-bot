{
  "name": "raya-discord-bot",
  "private": true,
  "author": "limitlesspc",
  "repository": {
    "type": "git",
    "url": "https://github.com/limitlesspc/raya-discord-bot.git"
  },
  "scripts": {
    "setup": "pnpm i && pnpm run setup:prisma",
    "setup:prisma": "prisma generate",
    "update": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "start": "node -r @swc-node/register src/main.ts",
    "deploy": "node -r @swc-node/register src/deploy.ts",
    "deploy:prod": "NODE_ENV=production node -r @swc-node/register src/deploy.ts",
    "lint": "eslint ."
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "@discordjs/voice": "^0.10.0",
    "@googleapis/customsearch": "^0.3.2",
    "@googleapis/youtube": "^6.0.0",
    "@limitlesspc/limitless": "^0.0.2",
    "@napi-rs/canvas": "^0.1.25",
    "@prisma/client": "4.0.0",
    "chalk": "4",
    "cheerio": "1.0.0-rc.12",
    "discord-api-types": "^0.36.1",
    "discord.js": "13.8.1",
    "dotenv": "^16.0.1",
    "genius-lyrics": "^4.3.8",
    "libsodium-wrappers": "^0.7.10",
    "owofy": "1.0.2",
    "undici": "^5.6.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.1",
    "@types/node": "18.0.3",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "prisma": "4.0.0",
    "typescript": "4.7.4"
  }
}
